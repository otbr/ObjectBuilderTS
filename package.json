{
  "name": "ironcore-object-builder",
  "version": "1.0.0",
  "description": "Object Builder for Ironcore - TypeScript version",
  "main": "dist/electron/electron/main.js",
  "scripts": {
    "build": "npm run build:backend && npm run build:ui && npm run build:electron",
    "build:backend": "tsc",
    "build:ui": "vite build",
    "build:electron": "tsc -p electron/tsconfig.json",
    "rebuild": "electron-rebuild -f -w canvas",
    "rebuild:all": "electron-rebuild",
    "postinstall": "electron-rebuild -f -w canvas",
    "dev": "tsc --watch",
    "dev:ui": "vite",
    "dev:electron": "concurrently \"npm run dev:ui\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "node dist/main.js",
    "start:electron": "electron .",
    "cli": "tsc && node dist/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "canvas": "^2.11.2",
    "electron": "^27.0.0",
    "lzma-native": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.34.5",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/sharp": "^0.32.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "electron-rebuild": "^3.2.9",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "wait-on": "^7.0.1"
  }
}
